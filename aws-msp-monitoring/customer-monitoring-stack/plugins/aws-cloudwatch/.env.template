# Environment variables for aws-cloudwatch
# Copy this to .env and customize

PLUGIN_NAME=aws-cloudwatch
PLUGIN_ENABLED=true

# Service Configuration
SERVICE_PORT=9214
SERVICE_IMAGE=alpine:latest

SERVICE_NAME=aws-cloudwatch
SERVICE_ENABLED=true

# Monitoring Configuration
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30s
LOG_LEVEL=INFO

# Performance Settings
MAX_CONNECTIONS=100
TIMEOUT_SECONDS=30
RETRY_ATTEMPTS=3
