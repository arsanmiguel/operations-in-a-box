# Environment variables for palo-alto-prisma
# Copy this to .env and customize

PLUGIN_NAME=palo-alto-prisma
PLUGIN_ENABLED=true

# Service Configuration
SERVICE_PORT=9216
SERVICE_IMAGE=alpine:latest

SERVICE_NAME=palo-alto-prisma
SERVICE_ENABLED=true

# Monitoring Configuration
METRICS_ENABLED=true
HEALTH_CHECK_INTERVAL=30s
LOG_LEVEL=INFO

# Performance Settings
MAX_CONNECTIONS=100
TIMEOUT_SECONDS=30
RETRY_ATTEMPTS=3
